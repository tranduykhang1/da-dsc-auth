{
  "name": "da-dsc-auth",
  "version": "2.1.1",
  "description": "This is a package for calling to DSC service (For internal)",
  "main": "dist/index",
  "type": "dist/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/tranduykhang1/da-dsc-auth"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Cristian Tran",
  "license": "ISC",
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "axios": "^1.4.0",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
